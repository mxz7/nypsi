{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": ["jsdoc"],
    "rules": {
        "no-unused-vars": "error",
        "jsdoc/no-undefined-types": 1,
        "indent": "off",
        "linebreak-style": "off",
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "never"
        ],
        "no-unreachable": "warn",
        "no-case-declarations": "off"
    }
}
